{
  "name": "tikozap-frontend",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate --schema=prisma/schema.prisma && NEXT_DISABLE_ESLINT=1 next build",
    "start": "next start",
    "lint": "next lint",
    "check": "npm run lint && npm run build",
    "eval:support": "tsx scripts/eval-support-assistant.ts",
    "eval:support:strict": "EVAL_STRICT=1 npm run eval:support",
    "smoke:twilio:webhook": "tsx scripts/twilio-webhook-smoke.ts",
    "smoke:twilio:webhook:dry": "tsx scripts/twilio-webhook-smoke.ts --dry-run",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.4.0",
    "@prisma/client": "^7.4.0",
    "better-sqlite3": "^12.5.0",
    "dotenv": "^17.2.3",
    "lucide-react": "0.452.0",
    "next": "^14.2.35",
    "openai": "^6.10.0",
    "pg": "^8.18.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "twilio": "^5.12.2"
  },
  "devDependencies": {
    "@types/node": "20.11.30",
    "@types/pg": "^8.16.0",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "autoprefixer": "10.4.18",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.4",
    "postcss": "8.4.33",
    "prisma": "^7.4.0",
    "svgo": "^4.0.0",
    "tailwindcss": "3.4.3",
    "tsx": "^4.21.0",
    "typescript": "5.4.5"
  }
}
